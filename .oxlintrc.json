{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/npm/oxlint/configuration_schema.json",
  "ignorePatterns": [
    "node_modules",
    "dist"
  ],
  "rules": {
    "no-unused-vars": "warn"
  },
  "categories": {
    "correctness": "error",
    "nursery": "off",
    "pedantic": "off",
    "restriction": "off",
    "style": "off",
    "suspicious": "warn"
  }
}
